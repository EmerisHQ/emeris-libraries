{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "homepage_url": "http://localhost:8080/",
  "description": "A Vue Browser Extension",
  "default_locale": "en",
  "permissions": [
    "activeTab",
    "storage",
    "<all_urls>",
    "*://*/*",
    "notifications",
    "identity"
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "192": "icons/192.png",
    "256": "icons/256.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'wasm-eval' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "/inject-emeris.js"
  ],
  "browser_action": {
    "browser_style": true,
    "default_popup": "popup.html?browser=true",
    "default_title": "Emeris Extension",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "version": "0.1.0"
}